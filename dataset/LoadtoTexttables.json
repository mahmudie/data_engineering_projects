{
	"name": "LoadtoTexttables",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Schemaname": {
				"type": "string"
			},
			"Tablename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{concat(dataset().Tablename,'.txt')}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@{concat(dataset().Schemaname,'/',dataset().Tablename)}",
					"type": "Expression"
				},
				"fileSystem": "bronze2"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}